<AIPackage Name="NajimiAttackReadyPackage" DefaultState="AI_RushReady">

    <AIState>
        <AI_RushReady UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.0" SearchIdentifier="Player">
            <Event_OnExecute>    
                <AIEvent Type="SetDirectionToTarget"/>
                <AIEvent Type="SetAction" Action="AC_RushReady"/>
            </Event_OnExecute>
            
            <Branch Condition="End" Execute="TerminatePackage"/>
        </AI_RushReady>

        <TerminatePackage>
            <Event_OnExecute>
                <AIEvent Type="TerminatePackage"/>
            </Event_OnExecute>
        </TerminatePackage>

    </AIState>

</AIPackage>