<AIPackage Name="TateDodgePackage" DefaultState="AI_Dodge">

    <AIState>
        <AI_Dodge UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.9" SearchIdentifier="Player">
            <Event_OnExecute>
                <AIEvent Type="SetDirectionToTarget" Angle="0"/>
                <AIEvent Type="SetAction" Action="AC_Dodge"/>
            </Event_OnExecute>

            <Branch Condition="End" Execute="TerminatePackage"/>
        </AI_Dodge>

        <TerminatePackage>
            <Event_OnExecute>
                <AIEvent Type="TerminatePackage"/>
            </Event_OnExecute>
        </TerminatePackage>

    </AIState>

</AIPackage>