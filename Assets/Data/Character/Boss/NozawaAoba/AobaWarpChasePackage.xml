<AIPackage Name="AobaWarpChasePackage" DefaultState="AI_Warp">

    <AIState>
        <AI_Warp UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.9" SearchIdentifier="Player">
            <Event_OnExecute>
                <AIEvent Type="SetDirectionToTarget"/>
                <AIEvent Type="SetAction" Action="AC_Warp"/>
            </Event_OnExecute>

            <Event_OnFrame>
                <AIEvent Type="SetDirectionToTarget"/>
            </Event_OnFrame>

            <Branch Condition="AttackSuccess" Execute="AI_Warpped"/>
        </AI_Warp>
        
        <AI_Warpped UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.9" SearchIdentifier="Player">
            <Event_OnExecute>
                <AIEvent Type="SetDirectionToTarget"/>
                <AIEvent Type="SetAction" Action="AC_Warpped"/>
            </Event_OnExecute>

            <Branch Condition="End" Execute="TerminatePackage"/>
        </AI_Warpped>

        <TerminatePackage>
            <Event_OnExecute>
                <AIEvent Type="TerminatePackage"/>
            </Event_OnExecute>
        </TerminatePackage>

    </AIState>

</AIPackage>